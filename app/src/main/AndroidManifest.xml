<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.team.alwiscruise">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher_cruise"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_cruise_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activtiy.FAQActivity"></activity>
        <activity android:name=".Activtiy.MyBookingTicketDetailActivity" />
        <activity android:name=".Activtiy.AccountEditActivity" />
        <activity android:name=".Activtiy.DestinationAllActivtiy" />
        <activity android:name=".Activtiy.DestinationDetailActivity" />
        <activity android:name=".Activtiy.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>